<game_config>
  <game_key>warsow_win32</game_key>
  <protocol>lgsl</protocol>
  <lgsl_query_name>warsow</lgsl_query_name>
  <game_name>Warsow</game_name>
  <server_exec_name>wsw_server_x86.exe</server_exec_name>
  <cli_template>%GAME_TYPE% %MAP% %IP% %PORT% %PLAYERS%</cli_template>
  <cli_params>
    <cli_param id="MAP" cli_string="+map" options="s" />
    <cli_param id="IP" cli_string="+set sv_ip" options="s" />
    <cli_param id="PORT" cli_string="+set sv_port" options="s" />
    <cli_param id="PLAYERS" cli_string="+set sv_maxclients" options="s" />
    <cli_param id="GAME_TYPE" cli_string="+set g_gametype" options="s" />
  </cli_params>
  <maps_location>basewsw/</maps_location>
  <max_user_amount>32</max_user_amount>
  <control_protocol>rcon</control_protocol>
  <control_protocol_type>old</control_protocol_type>
  <mods>
    <mod key="ca">
      <name>Clan Arena - ca</name>
    </mod>
    <mod key="ctf">
      <name>Capture The Flag</name>
    </mod>
    <mod key="da">
      <name>Clan Arena - da</name>
    </mod>
    <mod key="duel">
      <name>Duel</name>
    </mod>
    <mod key="ffa">
      <name>Free For All</name>
    </mod>
    <mod key="tdm">
      <name>Team Death Match</name>
    </mod>
  </mods>
</game_config>
